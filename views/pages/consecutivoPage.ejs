<%- include('../part/head', {title: "Consecutivos"}) %>
<%- include('../part/navbar', {rol_usu: global.__guest.rol_usu}) %>
<%- include('../part/menubar', {name: global.__guest.nombre_comp_usu, displayText: "N I Y", currentNode: "Homepage > Consecutivos"}) %>

<%#  Aca se podran generar los consecutivos para asignarlos como PK en las tablas [paises, aerolineas, puertas, compras, reservaciones (compras), vuelos] %>
<div class="container">

    <form>

        <div class="form-group row">
          <div class="col-sm-10">
            <label for="descripcionSelect">Descripción</label>
            <select class="form-control" id="descripcionSelect">
                <option>Países</option>
                <option>Aerolíneas</option>
                <option>Puertas de aeropuerto</option>
                <option>Compras o Reservas</option>
                <option>Vuelos</option>
            </select>
          </div>
        </div>

        <div class="form-group row">
          <label class="col-sm-2 col-form-label">Consecutivo</label>
          <div class="col-sm-10">
            <input type="text" class="form-control">
          </div>
        </div>

        <div class="form-group row">
            <div class="col-sm-2">Posee Prefijo?</div>
            <div class="col-sm-10">
              <div class="form-check">
                <input class="form-check-input" type="checkbox" id="gridCheck1">
                <label class="form-check-label" for="gridCheck1">
                  Si
                </label>
              </div>
            </div>
          </div>


          <div class="form-group row">
            <label class="col-sm-2 col-form-label">Prefijo</label>
            <div class="col-sm-10">
              <input type="text" class="form-control">
            </div>
          </div>


          <div class="form-group row">
            <div class="col-sm-2">Posee Rango?</div>
            <div class="col-sm-10">
              <div class="form-check">
                <input class="form-check-input" type="checkbox" id="gridCheck1">
                <label class="form-check-label" for="gridCheck1">
                  Si
                </label>
              </div>
            </div>
        </div>


        <div class="form-group row">
            <label class="col-sm-2 col-form-label">Rango Inicial</label>
            <div class="col-sm-10">
                <input type="text" class="form-control">
            </div>
        </div>


          <div class="form-group row">
            <label class="col-sm-2 col-form-label">Rango FInal</label>
            <div class="col-sm-10">
                <input type="text" class="form-control">
            </div>
        </div>
       

        <div class="form-group row">
          <div class="col-sm-10">
            <button type="submit" class="btn btn-success">Generar consecutivos</button>
          </div>
        </div>
    </form>
    

</div>

<%- include('../part/tail') %>
