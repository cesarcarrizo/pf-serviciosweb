<%- include('../part/head', {title: "Login"}) %>
<%- include('../part/navbarLogin') %>
   

<div class="container">
    <div class="jumbotron">
        <h1>VUELOS-V</h1>
        <hr>
        <p>Para ingresar al sistema por favor ingrese sus credenciales</p>
        <form method="POST" type="form-control">

            <div class="form-group">
              <label for="cedula_in">Cédula</label>
              <input name='cedula' type="text" class="form-control" id="cedula_in" aria-describedby="cedulaHelp" required>
              <small id="cedulaHelp" class="form-text text-muted">Nunca comparta su contraseña con nadie.</small>
            </div>

            <div class="form-group">
              <label for="passwd_in">Password</label>
              <input name='password' type="password" class="form-control" id="passwd_in" required>
            </div>

            <input type="submit" class="btn btn-success" value="Iniciar Sesión"/>

        </form>
    </div>   
</div>
    
<%- include('../part/tail') %>
